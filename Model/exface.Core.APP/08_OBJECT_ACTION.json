                                        "    },",
                                        "    \"repositories\": {",
                                        "        \"[#PACKAGE#]\": {",
                                        "            \"type\": \"composer\",",
                                        "            \"url\": \"[#=WorkbenchURL(\\'api/packagist/packages\\')#]\"",
